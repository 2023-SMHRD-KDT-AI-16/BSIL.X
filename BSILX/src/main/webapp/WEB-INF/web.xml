<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>IngreAllDayPriceChart</servlet-name>
		<display-name>IngreAllDayPriceChart</display-name>
		<description></description>
		<servlet-class>com.bsilx.controller.IngreAllDayPriceChart</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>FrontController</servlet-name>
		<display-name>FrontController</display-name>
		<description></description>
		<servlet-class>frontcontroller.FrontController</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>LogoutService</servlet-name>
		<display-name>LogoutService</display-name>
		<description></description>
		<servlet-class>com.bsilx.controller.LogoutService</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>IngreOneDayPriceChart</servlet-name>
		<display-name>IngreOneDayPriceChart</display-name>
		<description></description>
		<servlet-class>com.bsilx.controller.IngreOneDayPriceChart</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LboxPrint</servlet-name>
		<display-name>LboxPrint</display-name>
		<description></description>
		<servlet-class>com.bsilx.controller.LboxPrint</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>JoinService</servlet-name>
		<display-name>JoinService</display-name>
		<description></description>
		<servlet-class>com.bsilx.controller.JoinService</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Test</servlet-name>
		<display-name>Test</display-name>
		<description></description>
		<servlet-class>com.bsilx.controller.Test</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>IngreAllDayPriceChart</servlet-name>
		<url-pattern>/IngreAllDayPriceChart</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>LogoutService</servlet-name>
		<url-pattern>/LogoutService</url-pattern>
	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>IngreOneDayPriceChart</servlet-name>
		<url-pattern>/IngreOneDayPriceChart</url-pattern>
	</servlet-mapping>
	
	<servlet-mapping>
		<servlet-name>FrontController</servlet-name>
		<url-pattern>*.do</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LboxPrint</servlet-name>
		<url-pattern>/LboxPrint</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>JoinService</servlet-name>
		<url-pattern>/JoinService</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Test</servlet-name>
		<url-pattern>/Test</url-pattern>
	</servlet-mapping>

</web-app>
