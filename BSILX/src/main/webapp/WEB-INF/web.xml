<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>

  <display-name>Archetype Created Web Application</display-name>
  
   <servlet>
  	<servlet-name>IngrePriceChart</servlet-name>
  	<display-name>IngrePriceChart</display-name>
  	<description></description>
  	<servlet-class>com.bsilx.controller.IngrePriceChart</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>FrontController</servlet-name>
  	<display-name>FrontController</display-name>
  	<description></description>
  	<servlet-class>frontcontroller.FrontController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>memberDTO</servlet-name>
  	<display-name>memberDTO</display-name>
  	<description></description>
  	<servlet-class>com.bsilx.model.memberDTO</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>logoutService</servlet-name>
  	<display-name>logoutService</display-name>
  	<description></description>
  	<servlet-class>com.bsilx.controller.logoutService</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>IngrePriceChart</servlet-name>
  	<url-pattern>/IngrePriceChart</url-pattern>
  </servlet-mapping>
  
	
  
  
  <servlet-mapping>
  	<servlet-name>memberDTO</servlet-name>
  	<url-pattern>/memberDTO</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>logoutService</servlet-name>
  	<url-pattern>/logoutService</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>FrontController</servlet-name>
  	<url-pattern>*.do</url-pattern>
  </servlet-mapping>
 


</web-app>
